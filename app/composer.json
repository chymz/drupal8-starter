{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/admin_toolbar": "^1.22",
        "drupal/better_formats": "1.x-dev",
        "drupal/cdn": "^3.2",
        "drupal/ckeditor_media_embed": "^1.1",
        "drupal/config_split": "^1.3",
        "drupal/config_update": "^1.5",
        "drupal/console": "^1.0.2",
        "drupal/consumers": "^1.0@beta",
        "drupal/core": "~8.4",
        "drupal/crop": "^1.5",
        "drupal/ctools": "^3.0",
        "drupal/devel": "^1.2",
        "drupal/diff": "^1.0@RC",
        "drupal/draggableviews": "^1.0",
        "drupal/dropzonejs": "^2.0@alpha",
        "drupal/embed": "^1.0",
        "drupal/entity_block": "^1.0@beta",
        "drupal/entity_browser": "^1.4",
        "drupal/entity_embed": "^1.0@beta",
        "drupal/features": "^3.5",
        "drupal/field_group": "^1.0",
        "drupal/google_analytics": "^2.2",
        "drupal/graphql": "^3.0@beta",
        "drupal/image_widget_crop": "^2.1",
        "drupal/inline_entity_form": "^1.0@beta",
        "drupal/jsonapi": "^1.7",
        "drupal/media_entity": "^1.7",
        "drupal/media_entity_document": "^1.1",
        "drupal/media_entity_image": "^1.2",
        "drupal/media_entity_video": "^1.0@beta",
        "drupal/menu_admin_per_menu": "^1.0",
        "drupal/metatag": "^1.4",
        "drupal/module_filter": "^3.1",
        "drupal/openapi": "^1.0@alpha",
        "drupal/paragraphs": "^1.2",
        "drupal/pathauto": "^1.0",
        "drupal/redirect": "^1.0",
        "drupal/schemata": "^1.0@alpha",
        "drupal/search_api": "^1.6",
        "drupal/simple_oauth": "^3.5",
        "drupal/simple_sitemap": "^2.11",
        "drupal/token": "^1.1",
        "drupal/video_embed_field": "^1.5",
        "drush/drush": "^9.0.0",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "2.9.1",
        "webflo/drupal-core-require-dev": "~8.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        }
    }
}
